{
    "name": "vildan/portal",
    "description": "Vildan Portal - Okul YÃ¶netim Sistemi",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-mysqli": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "phpoffice/phpspreadsheet": "^1.29",
        "phpmailer/phpmailer": "^6.9",
        "google/apiclient": "^2.15",
        "monolog/monolog": "^2.9",
        "nesbot/carbon": "^2.72"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/"
        },
        "files": [
            "vendor/symfony/translation/Resources/functions.php",
            "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}